{
  "name": "express-session-example-starter",
  "version": "1.0.0",
  "description": "Implements express-sessions, simple session middleware for Express.js.\n\nIn this demo each visitor gets a unique session, allowing you to store the user state.",
  "main": "src/index.js",
  "scripts": {
    "dev": "concurrently --kill-others \"yarn start\" \"yarn view\"",
    "view" :"cd view && yarn start",
    "start": "nodemon src/index.js localhost 8080"
  },
  "dependencies": {
    "express": "4.17.1",
    "express-session": "1.17.0",
    "jsonwebtoken": "9.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "http-proxy": "^1.18.1",
    "nodemon": "1.18.4"
  },
  "keywords": [
    "express",
    "expressjs",
    "express-session"
  ]
}
